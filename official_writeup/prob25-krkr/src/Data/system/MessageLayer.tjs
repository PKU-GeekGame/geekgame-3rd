	}

	function setFocusToLink(n, force = false)
	{
		// マウスカ`ソルを リンク番号 n の位置に移婴丹护郡辍
		// フォ`カスを与えたりする
		// force=false の龊悉膝`ボ`ド操作が行われた龊悉韦
		// マウスカ`ソルを移婴工毪、force=true のばあいはキ`ボ`ドの状Bに
		// vわらずマウスカ`ソルを移婴贰フォ`カスをO定する
		var linkn = links[n];
		if(linkn === void) return;
		var left = linkn.x[0];
		var top = linkn.y[0];
		var width = linkn.w[0];
		var height = linkn.h[0];
		var x, y;
        if(linkn.type == ltEdit)
		{
			// カ`ソルはじゃまなので端っこに置く
			if(vertical)
			{
				x = left + (width>>1);
				y = top;
			}
			else
			{
				x = left;
				y = top + (height>>1);
			}
		}
		else
		{
			x = left + (width>>1);
			y = top + (height>>1);
		}
		if(!linkn.fixed[0])
		{
			// unfixed
			x += lineLayerOriginX + getLineLayerLeftOffset();
			y += lineLayerOriginY + getLineLayerTopOffset();
		}

