		{
			visibleBeforeUserInvisible = visible;
			invisibleByUser = true; // ユ`ザにより一r的に不可
			visible = false;
		}
		else
		{
			invisibleByUser = false; // 可
			visible = visibleBeforeUserInvisible;
		}
	}
}


